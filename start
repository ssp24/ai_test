#!/bin/bash

# Source the .env file if it exists
if [ -f .env ]; then
  echo "Loading environment variables from .env file..."
  source .env
fi

# Export environment variables to be available for other scripts
export OPENAI_API_KEY

# Use the secret in your application
echo "Environment variable OPENAI_API_KEY is set"



echo "Starting JupyterLab with custom attributes..."
jupyter lab --AiExtension.default_language_model=anthropic-chat:claude-2.1
